{
    "name": "hyenergysolutions/freepbx-laravel",
    "description": "Laravel package for FreePBX GraphQL API integration",
    "keywords": ["laravel", "freepbx", "pbx", "voip", "graphql"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "HyEnergy Solutions",
            "email": "dev@hyenergysolutions.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/cache": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "HyEnergySolutions\\FreePBX\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyEnergySolutions\\FreePBX\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HyEnergySolutions\\FreePBX\\FreePBXServiceProvider"
            ],
            "aliases": {
                "FreePBX": "HyEnergySolutions\\FreePBX\\Facades\\FreePBX"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
